{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.json",
  "window_hiding_behaviour": "Cloak",
          "default_workspace_padding": 1,
  "cross_monitor_move_behaviour": "Insert",
  "default_container_padding": 1,
  "border": true,
  "border_width": 2,
  "border_offset": 0,
  "theme": {
    "palette": "Base16",
    "name": "Ashes",
    "unfocused_border": "Base03",
    "bar_accent": "Base04",
    "floating_border": "Base06",
    "monocle_border": "Base06",
    "single_border": "Base06"
  },
  "animation": {
    "enabled": true,
    "fps": 65,
    "duration": {
      "movement": 250,
      "transparency": 150
    },
    "style": {
      "movement": "EaseOutExpo",
      "transparency": "Linear"
    }
  },
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP"
        },
        {
          "name": "II",
          "layout": "VerticalStack"
        },
        {
          "name": "III",
          "layout": "Grid"
        },
        {
          "name": "IV",
          "layout": "RightMainVerticalStack"
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "Personal",
          "layout": "BSP"
        },
        {
          "name": "Work",
          "layout": "VerticalStack"
        },
        {
          "name": "School",
          "layout": "VerticalStack"
        }
      ]
    }
  ],
  "transparency_alpha": 240,
  "transparency": false
}
