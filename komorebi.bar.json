{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.bar.json",
  "monitor": 1,
  "font_family": "JetBrains Mono",
  "grouping": {
    "kind": "None",
    "style": "DefaultWithShadowB4O1S3"
  },
  "transparency_alpha":10,
  "height": 32,
  "icon_scale": 1.2,
  "theme": {
    "palette": "Base16",
    "name": "Ashes"
  },
  "left_widgets": [
    {
      "Komorebi": {
        "workspaces": {
          "rounding": 5,
          "enable": true,
          "hide_empty_workspaces": false
        },
        "focused_window": {
          "enable": true,
          "show_icon": true,
          "display":"IconAndText"
        }
      }
    },
    {
      "Media": {
        "enable": true
      }
    }
  ],
  "center_widgets": [
    {
      "Media": {
        "enable": false
      }
    }
  ],
  "right_widgets": [
    {
      "Komorebi": {
        "layout": {
          "enable": true,
          "display":"Icon"
        }
      }
    },
    {
      "Update": {
        "enable": false
      }
    },
    {
      "Storage": {
        "enable": false
      }
    },
    {
      "Cpu": {
        "enable": true
      }
    },
    {
      "Memory": {
        "enable": true
      }
    },
    {
      "Network": {
        "enable": false,
        "show_total_data_transmitted": false,
        "show_network_activity": false
      }
    },
    {
      "Date": {
        "enable": true,
        "format": "DayDateMonthYear"
      }
    },
    {
      "Time": {
        "enable": true,
        "format": "TwelveHourWithoutSeconds"
      }
    },
    {
      "Battery": {
        "enable": false
      }
    }
  ]
}
